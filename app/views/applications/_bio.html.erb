<%= form_with url: "/applications/#{@application.id}",  method: :patch, local: true do |form| %>
  <%= form.label 'Why I would be a good owner:' %><br/>
  <%= form.text_field :applicant_bio, required: true, :size=>"100", :minlength=>"5", :maxlength=>"250" %>
  <%= form.submit 'Submit Application' %>
<% end %>
