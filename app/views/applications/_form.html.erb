<%= form_with url: path, method: method, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, required: true %><br/>
  <%= form.label :street_address %>
  <%= form.text_field :street_address, required: true %><br/>
  <%= form.label :city %>
  <%= form.text_field :city, required: true %>
  <%= form.label :state %>
  <%= form.text_field :state, required: true %>
  <%= form.label :zip_code %>
  <%= form.text_field :zip_code, required: true %><br/>
  <%= form.submit 'Start Application'%>
<% end %>
