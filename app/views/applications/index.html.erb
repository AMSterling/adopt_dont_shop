<h1>Application</h1>

<% @applications.each_with_index do |application, index| %>
<div id="application-<%= index %>"
  <h3><%= application.name %></h3>
  <p>
    <%= application.street_address %>
    <%= application.city %>
    <%= application.state %>
    <%= application.zip_code %>
  </p>
</div>
<% end %>
